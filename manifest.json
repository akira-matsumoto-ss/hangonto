{
  "manifest_version": 2,
  "name": "Hangonto",
  "version": "1.0",

  "icons": { "128": "icon.png" },

  "permissions":["tabs", "declarativeContent"],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "page_action": {
    "default_title": "Hangonto",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ]
}

